@{
    ViewData["Title"] = "Home Page";
}

<div class="p-3 ms-5">
    <h1 class="display-6 ms-2">Cadastro Empresa</h1>

    <div class="row col-12 d-flex">

        <div class="col-6 p-2" hidden>
           <form method="post" id="Form-CadEmpresa">        
               <div class="col-12 row ms-1">
                    <div class="col-12 mt-3">
                        <label for="nome">Nome da Empresa</label>
                       <input type="text" class="form-control" name="nome" id="nome"  required />
                   </div>
                    <div class="col-12 mt-3">
                       <label for="cnpj">CNPJ da Empresa</label>
                       <input type="text" class="form-control" title="Preencha O CNPJ somente com numeros" name="cnpj" id="cnpj" data-mascara="cnpj" maxlength="18" placeholder="01234567890123" required onclick="aplyMaskCNPJ(this)" />
                   </div>

                    <div class="col-12 mt-3">
                       <label for="faturamento">Faturamento da Empresa</label>
                       <input type="text" class="form-control" name="faturamento" id="faturamento" required />
                   </div>

                    <div class="col-12 mt-3">
                       <label for="ramo">Ramo da Empresa</label>
                       <select class="form-control form-select" id="ramo" name="ramo" required>
                           <option value="0" selected>Selecione</option>
                           <option value=1>Produto</option>
                           <option value=2>Serviços</option>
                      </select>
                   </div>
           
                   <div class="col-8 mt-3">
                      <button type="submit" class="btn btn-dark">Salvar</button>
                   </div>
               </div>
           </form>
        </div>

        <div class="col-7 card ms-4 p-3 heightCustom">
